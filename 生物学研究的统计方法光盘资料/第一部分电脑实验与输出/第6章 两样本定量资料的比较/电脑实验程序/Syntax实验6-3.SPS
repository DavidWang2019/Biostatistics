COMPUTE d = control - newdrug .
EXECUTE .
EXAMINE
  VARIABLES=d
  /PLOT  NPPLOT
  /STATISTICS DESCRIPTIVES
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.
T-TEST
  PAIRS = control  WITH newdrug (PAIRED)
  /CRITERIA = CI(.95)
  /MISSING = ANALYSIS.
T-TEST
  /TESTVAL = 0
  /MISSING = ANALYSIS
  /VARIABLES = d
  /CRITERIA = CI(.95) .
