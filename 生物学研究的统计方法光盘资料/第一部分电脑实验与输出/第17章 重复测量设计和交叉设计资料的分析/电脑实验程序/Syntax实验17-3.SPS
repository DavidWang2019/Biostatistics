INPUT PROGRAM.
LOOP #I=1 TO 200.
COMPUTE aaa0 = 1.
END CASE.
END LOOP.
END FILE.
END INPUT PROGRAM.
CREATE
  /id=CSUM(aaa0).
COMPUTE group= TRUNC((id-1)/100) .
COMPUTE y10 = RV.NORMAL(0,1)*2 .
COMPUTE y1 = RV.NORMAL(0,1)*10+y10.
COMPUTE y2 = RV.NORMAL(0,1)*10+y10.
COMPUTE y3 = RV.NORMAL(0,1)*10+y10.
EXECUTE .
GLM
  y1 y2 y3  BY group
  /WSFACTOR = factor1 3 Polynomial
  /METHOD = SSTYPE(3)
  /CRITERIA = ALPHA(.05)
  /WSDESIGN = factor1
  /DESIGN = group .
