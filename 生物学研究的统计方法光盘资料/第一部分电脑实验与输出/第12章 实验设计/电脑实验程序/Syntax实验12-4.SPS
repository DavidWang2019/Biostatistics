SET
  SEED=12345.
COMPUTE RAND = UNIFORM(100) .
EXECUTE .
SORT CASES BY
  sex (A) .
RANK
  VARIABLES=RAND  (A) BY sex  /RANK /PRINT=YES
  /TIES=MEAN .
RECODE
  RRAND
  (1 thru 25=1)  (26 thru 50=2)  INTO  GROUP .
EXECUTE .
T-TEST
  GROUPS = GROUP(1 2)
  /MISSING = ANALYSIS
  /VARIABLES = height age
  /CRITERIA = CI(.95) .
