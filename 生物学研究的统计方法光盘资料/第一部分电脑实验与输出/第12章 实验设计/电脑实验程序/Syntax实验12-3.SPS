SET
  SEED=12345.
COMPUTE RAND = UNIFORM(100) .
EXECUTE .
AUTORECODE
  VARIABLES=RAND  /INTO RANK
  /PRINT.
RECODE
  RANK
  (1 thru 50=1)  (51 thru 100=2)  INTO  Group .
EXECUTE .
T-TEST
  GROUPS = Group(1 2)
  /MISSING = ANALYSIS
  /VARIABLES = height age
  /CRITERIA = CI(.95) .
CROSSTABS
  /TABLES=Group  BY sex
  /FORMAT= AVALUE TABLES
  /STATISTIC=CHISQ
  /CELLS= COUNT
  /COUNT ROUND CELL .

